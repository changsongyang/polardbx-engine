3. Privileges
-- Restore table need ALTER_ACL | DROP_ACL of origin table and
INSERT_ACL | CREATE_ACL of dest table;
------------------------------------------------------
set global recycle_scheduler=off;
use db_recycle;
create table t1(id int);
create table t2(id int);
drop table t1;
drop table t2;
call dbms_recycle.show_tables();
