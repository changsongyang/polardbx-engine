__recycle_bin__	__innodb_165	db_recycle	t1	#	#
__recycle_bin__	__innodb_166	db_recycle	t2	#	#
call dbms_recycle.restore_table("__innodb_165", "db_recycle_2", "t1");
ERROR 42000: INSERT, CREATE command denied to user 'normal_1'@'localhost' for table 't1'
call dbms_recycle.show_tables();
SCHEMA	TABLE	ORIGIN_SCHEMA	ORIGIN_TABLE	RECYCLED_TIME	PURGE_TIME
__recycle_bin__	__innodb_165	db_recycle	t1	#	#
__recycle_bin__	__innodb_166	db_recycle	t2	#	#
call dbms_recycle.restore_table("__innodb_165", "db_recycle_2", "t1");
ERROR 42000: DROP, ALTER command denied to user 'normal_2'@'localhost' for table '__innodb_165'
call dbms_recycle.show_tables();
SCHEMA	TABLE	ORIGIN_SCHEMA	ORIGIN_TABLE	RECYCLED_TIME	PURGE_TIME
__recycle_bin__	__innodb_165	db_recycle	t1	#	#
__recycle_bin__	__innodb_166	db_recycle	t2	#	#
call dbms_recycle.restore_table("__innodb_165", "db_recycle_2", "t2");
ERROR 42000: INSERT, CREATE command denied to user 'normal_3'@'localhost' for table 't2'
call dbms_recycle.show_tables();
SCHEMA	TABLE	ORIGIN_SCHEMA	ORIGIN_TABLE	RECYCLED_TIME	PURGE_TIME
__recycle_bin__	__innodb_165	db_recycle	t1	#	#
__recycle_bin__	__innodb_166	db_recycle	t2	#	#
call dbms_recycle.restore_table("__innodb_166", "db_recycle_2", "t1");
call dbms_recycle.show_tables();
SCHEMA	TABLE	ORIGIN_SCHEMA	ORIGIN_TABLE	RECYCLED_TIME	PURGE_TIME
__recycle_bin__	__innodb_165	db_recycle	t1	#	#
call dbms_recycle.restore_table("__innodb_165", "db_recycle_2", "t2");
use db_recycle_2;
show tables;
